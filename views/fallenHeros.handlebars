<div class="wrapper">

    <h1>The Fallen</h1>

    {{! View all of the service members that currently exist in the database }}
    <div class="flex-container">
        {{#each servicemembers}}
        <div class="card">
            <img src="{{res.name}}" alt="Photo of {{this.dataValues.first_name}} {{this.dataValues.last_name}}">
            <div class="card-section">
                <h2>{{this.res.name}}</h2>
             
            </div>
        </div>
        {{/each}}
    </div>
    
    <!-- Footer with links to other pages -->
    <footer>
        <div class="container" id="menu-bar">    
            <nav>
                <ul class="menu menu-hover-lines">
                    <li><a href="/">Home</a></li>
                    <li><a href="/SEALs">Fallen Service Members</a></li>
                    <li><a href="#">Quotes</a></li>
                    <li><a href="#">Links</a></li>
                    <li><a href="/creed">Creeds</a></li>
                    <li><a href="/about">Contact Us</a></li>
                </ul>
            </nav>
        </div>

        <blockquote id="quote">"To us, and those like us, damn few."</blockquote>
    </footer>
</div>